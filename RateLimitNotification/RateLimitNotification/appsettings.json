{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimitConfiguration": {
    "NewsConfiguration": {
      "MaxAmount": 1,
      "ExpiryTimeInMin": 1440
